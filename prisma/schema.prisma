generator client {
  provider        = "prisma-client-js"
  output          = "../generated/prisma"
  previewFeatures = ["relationJoins", "typedSql"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model contract_data {
  contract_id     String?
  ledger_sequence Int
  key_hash        String?
  durability      String?
  key_symbol      String?
  key             Bytes?
  val             Bytes?
  closed_at       DateTime @db.Timestamptz(6)
  pk_id           BigInt   @id @default(autoincrement())

  @@index([contract_id], map: "idx_contract_id")
  @@index([key_hash], map: "idx_key_hash")
  @@index([key_symbol], map: "idx_key_symbol")
}

model gorp_migrations {
  id         String    @id
  applied_at DateTime? @db.Timestamptz(6)
}

model ttl {
  key_hash                   String?
  ledger_sequence            Int
  live_until_ledger_sequence Int
  closed_at                  DateTime @db.Timestamptz(6)
  pk_id                      BigInt   @id @default(autoincrement())
}
