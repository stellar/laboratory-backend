{
  "name": "stellar-lab-api",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/stellar/laboratory-backend",
  "author": "Stellar Development Foundation <hello@stellar.org>",
  "license": "MIT",
  "engines": {
    "node": ">=22.0.0"
  },
  "packageManager": "pnpm@10.14.0",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "dev:watch": "nodemon --watch src --ext ts --exec ts-node src/index.ts",
    "prisma:generate": "npx prisma generate",
    "migrate": "npx prisma migrate dev",
    "prisma:studio": "npx prisma studio",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky"
  },
  "dependencies": {
    "@google-cloud/cloud-sql-connector": "^1.9.0",
    "@sentry/node": "^10.39.0",
    "@stellar/stellar-sdk": "^14.5.0",
    "cors": "^2.8.6",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "pino": "^10.3.1",
    "pino-http": "^11.0.0",
    "proxy-addr": "^2.0.7",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@prisma/client": "^6.19.0",
    "@testcontainers/postgresql": "^11.12.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.2.3",
    "@types/proxy-addr": "^2.0.3",
    "eslint": "^9.39.1",
    "globals": "^17.3.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "lint-staged": "^16.2.7",
    "nodemon": "^3.1.11",
    "pino-pretty": "^13.1.3",
    "prettier": "^3.8.1",
    "prisma": "^6.19.0",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.0"
  },
  "pnpm": {
    "overrides": {
      "test-exclude": "7.0.0",
      "safer-buffer": "npm:buffer@^6.0.3",
      "js-yaml": "^4.1.1",
      "glob": "^13.0.0"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint",
      "prettier --check"
    ]
  }
}
